{
  "info": {
    "name": "Sample API Collection",
    "description": "A sample collection to test Postman import",
    "_postman_id": "12345678-1234-1234-1234-123456789012",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Get Users",
      "request": {
        "method": "GET",
        "header": [
          {
            "key": "Authorization",
            "value": "Bearer {{token}}",
            "type": "text"
          }
        ],
        "url": {
          "raw": "https://api.example.com/users",
          "protocol": "https",
          "host": ["api", "example", "com"],
          "path": ["users"]
        },
        "description": "Retrieve all users from the API"
      }
    },
    {
      "name": "Create User",
      "request": {
        "method": "POST",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          },
          {
            "key": "Authorization",
            "value": "Bearer {{token}}"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"role\": \"user\"\n}"
        },
        "url": {
          "raw": "https://api.example.com/users",
          "protocol": "https",
          "host": ["api", "example", "com"],
          "path": ["users"]
        },
        "description": "Create a new user"
      }
    },
    {
      "name": "Update User",
      "request": {
        "method": "PUT",
        "header": [
          {
            "key": "Content-Type",
            "value": "application/json"
          }
        ],
        "body": {
          "mode": "raw",
          "raw": "{\n  \"name\": \"Jane Doe\",\n  \"email\": \"jane@example.com\"\n}"
        },
        "url": {
          "raw": "https://api.example.com/users/{{userId}}",
          "protocol": "https",
          "host": ["api", "example", "com"],
          "path": ["users", "{{userId}}"]
        }
      }
    },
    {
      "name": "Delete User",
      "request": {
        "method": "DELETE",
        "url": {
          "raw": "https://api.example.com/users/{{userId}}",
          "protocol": "https",
          "host": ["api", "example", "com"],
          "path": ["users", "{{userId}}"]
        },
        "description": "Delete a user by ID"
      }
    }
  ]
}